import java.util.Scanner;
public class rev
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.print("enter the number");
        int a=s.nextInt();
        int b=0;
        for(int i=1;i<=a/2;i++)
        {
            if(a%i==0)
            b=b+i;
        }
        if(a==b)
        {
            System.out.print("it's a perfect number");
        }else{
            System.out.print("its");
            s.close();
        }
      
    }
}
