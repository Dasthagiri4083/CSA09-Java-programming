public class rev
{
    public static void main(String[] args)
    {
      Scanner s=new Scanner(System.in);
      System.out.print("enter the number");
      int a=s.nextInt();
      System.out.print("enter the number");
      int b=s.nextInt();
      System.out.print("enter the number");
      int c=s.nextInt();
      for(int d=a;d<=b;d=d+c+1)
      {
        System.out.print(d+ " ");
      }
      s.close();

    }
}
