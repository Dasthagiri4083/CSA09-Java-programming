import java.util.Scanner;
class pog
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.print("enter the number");
		int a=s.nextInt();
		int sum=0;
		while(a!=0)
		{
			int rem=a%10;
			sum=sum+rem;
			a=a/10;
		}
		System.out.print("sum"+sum);
		s.close();
	}
}
