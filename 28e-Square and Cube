import java.util.Scanner;
public class rev
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.print("enter the number");
        int a=s.nextInt();
        System.out.println("square:"+(a*a));
        System.out.print("cube:"+(a*a*a));
        s.close();
    }
}
