import java.util.Scanner;
public class rev
{
    public static void main(String[] args)
    {
      Scanner s=new Scanner(System.in);
      System.out.print("enter the number");
      int n=s.nextInt();
      int a=1;
      for(int i=2;i<=n;i++)
      {
        a=a*i;
        System.out.print(a+" ");
        s.close();
      }
      
    }
}
