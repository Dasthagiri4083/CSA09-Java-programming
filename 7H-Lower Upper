import java.util.Scanner;
public class rev
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        System.out.print("enter the lower ");
        int a=s.nextInt();
        System.out.print("enter the upper");
        int b=s.nextInt();
        for(int i=a;i<=b;i++)
        {
            System.out.println ("("+i+","+(i*i)+")");
        }
s.close();
    }
}
