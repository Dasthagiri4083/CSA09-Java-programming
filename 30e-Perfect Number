import java.util.Scanner;
class pog
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
		System.out.print("enter the number");
		int a=s.nextInt();
		int fact=0;
		for(int i=1;i<a;i++)
		{
			if(a%i==0)
				fact=fact+i;
		}
		if(a==fact)
		{
			System.out.print("perfect number");
		}else{
			System.out.print("not a perfect number");
			}
		s.close();
	}

}
